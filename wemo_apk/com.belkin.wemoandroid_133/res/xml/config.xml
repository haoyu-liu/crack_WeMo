<?xml version="1.0" encoding="utf-8"?>
<widget id="io.cordova.helloCordova" version="2.0.0" xmlns="http://www.w3.org/ns/widgets">
    <name>Belkin WeMo</name>
    <allow-navigation href="http://*" />
    <allow-navigation href="https://*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="mailto:*" launch-external="yes" />
    <access origin="*" />
    <content src="index.html" />
    <preference name="useBrowserHistory" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="load-url-timeout" value="70000" />
    <preference name="backgroundColor" value="#73d44c" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
    </feature>
    <feature name="Capture">
        <param name="android-package" value="org.apache.cordova.mediacapture.Capture" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.liteglue.SQLitePlugin" />
    </feature>
    <feature name="SetupPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.SetupPlugin" />
    </feature>
    <feature name="DevicePlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.DevicePlugin" />
    </feature>
    <feature name="RulesPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.RulesPlugin" />
    </feature>
    <feature name="WebServicePlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.WebServicePlugin" />
    </feature>
    <feature name="NativeUtilPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.NativeUtilPlugin" />
    </feature>
    <feature name="SharedPreferencesPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.AndroidPreferences" />
    </feature>
    <feature name="StartAppPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.StartApp" />
    </feature>
    <feature name="WeMoSMARTDevicePlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.WeMoSMARTDevicePlugin" />
    </feature>
    <feature name="WeMoSMARTUIPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.WeMoSMARTUIPlugin" />
    </feature>
    <feature name="CordovaHttpPlugin">
        <param name="android-package" value="com.synconset.CordovaHttpPlugin" />
    </feature>
    <feature name="SmartDevicePlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.SmartDevicePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="AppPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.AppPlugin" />
    </feature>
    <feature name="AppSettingsPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.AppSettingsPlugin" />
    </feature>
    <feature name="SensorEventsPlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.SensorEventsPlugin" />
    </feature>
    <feature name="FirmwareUpdatePlugin">
        <param name="android-package" value="com.belkin.cordova.plugin.FirmwareUpdatePlugin" />
    </feature>
</widget>